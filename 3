#include <stdio.h>
int main()
{
    int a,b,c,d,n=0;
    for(int i=11; i<=999; i++)
    {
        a=i/100;
        b=i/10%10;
        c=i%10;
        d=i/10;
        if((a==c && i>=100) || (d==c && i<100))
        {
            n+=1;
            printf("%d\n",i);
        }
    }
    printf("%d", n);
} 
